{
  "name": "cue_asap_web",
  "version": "1.0.0",
  "homepage": "https://CUE-ASAP.github.io/CUE-WebApp-raw",
  "description": "Cue ASAP - Society's Helping Hand",
  "main": "server.js",
  "scripts": {
    "client-install": "cd client && cd reactjs && npm install",
    "server": "nodemon server.js",
    "client": "cd client && cd reactjs && npm start",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "build": "react-scripts build",
    "start": "react-scripts start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false cd client && cd reactjs && npm install && npm run build"
  },
  "author": "cue_guardian",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^4.5.3",
    "concurrently": "^5.3.0",
    "config": "^3.3.3",
    "dotenv": "^8.2.0",
    "emailjs-com": "^2.6.4",
    "express": "^4.17.1",
    "gh-pages": "^3.1.0",
    "google-maps-react": "^2.0.6",
    "jsonwebtoken": "^8.5.1",
    "lottie-web": "^5.7.6",
    "mapbox-gl": "^2.1.1",
    "mongoose": "^5.11.8",
    "nodemailer": "^6.4.17",
    "react-countdown-circle-timer": "^2.5.0",
    "react-map-gl": "^6.1.7",
    "react-redux": "^7.2.2",
    "react-toastify": "^6.2.0",
    "react-transition-group": "^4.4.1",
    "reactstrap": "^8.7.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.6"
  }
}
